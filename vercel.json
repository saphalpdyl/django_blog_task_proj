{
    "name": "blog-app",
    "version": "3.10",
    "build": {
      "commands": [
        "pip install -r requirements.txt",
        "python manage.py collectstatic",
        "python manage.py collectstatic"
      ]
    },
    "routes": [
      {
        "src": "/static/*",
        "dest": "/static/*"
      },
      {
        "src": "/(.*)",
        "dest": "/wsgi.py"
      }
    ]
  }